pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- night-sky
-- by jillukowicz

--globals
local star = {x=nil, y=nil, c=nil, clr=6}
local moon = {x=nil, y=nil, r=10, c=7}
local cloud ={x=nil, y=nil, r=nil}
local stars = {}
local camera_pos = {x=0, y=0}
local bg_clouds = {}
local fg_clouds = {}
local time = 0
local speed = 1

--star object
function star:new(o)
  o = o or {}
	o.x = flr(rnd(128))
	o.y = flr(rnd(128))
	o.clr = flr(rnd(16))
	o.c = cocreate(function() o:shine() end)
  setmetatable(o, self)
  self.__index = self
  return o
end

function star:draw()
	pset(self.x, self.y, self.clr)
end

function star:shine()
	while true do
	  self.clr = 5 + flr(rnd(2))
	  delay(flr(rnd(50)))
	end
end
--end star object

--cloud object
function cloud:new(o)
  o = o or {}
  setmetatable(o, self)
  self.__index = self
  return o
end

function cloud:draw(cl)
  circfill(self.x, self.y, self.r, cl)
end
--end cloud object

--moon object
function moon:new(o)
  o = o or {}
  setmetatable(o, self)
  self.__index = self
  return o
end

function moon:draw()
  circfill(self.x, self.y, self.r, self.c)
  pset(self.x+5, self.y-5, 5)
  pset(self.x+2, self.y+9, 5)
  pset(self.x-7, self.y+2, 5)
end
--end moon object

-- inits
function init_clouds()
	for i=0,10 do
		--add(bg_clouds, {x = i * 8, y = rnd(8), r = rnd(8) + 6})
		add(bg_clouds, cloud:new({x = i * 8, y = 110 - rnd(16), r = rnd(8) + 6}))
		--add(fg_clouds, {x = i * 8, y = rnd(8) - 4, r = rnd(8) + 4})
		add(fg_clouds, cloud:new({x = i * 8, y = 110 - rnd(16), r = rnd(8) + 4}))
	end
end

function init_moon()
  moon = moon:new({x=flr(rnd(32)), y=flr(rnd(32))})
end

function init_stars()
  for i = 1,50 do
		add(stars, star:new())
	end
end
--end inits

-- uitl
function delay(t)
  for i=1,t do
    yield()
  end
end

function outline(s,tc,oc,x,y)
    for i=-1,1 do
        for j=-1,1 do
            print(s,x+i,y+j,oc)
        end
    end
    print(s,x,y,tc)
end

function outline_centered(s,tc,oc)
  outline(s,tc,oc,64-#s*4/2,64)
end
--end util

--updates
function update_stars()
  for s in all(stars) do
    coresume(s.c)
  end
end

function update_clouds( )
	for c in all(bg_clouds) do
		c.x -= speed / 2
		if c.x <= -8 then
			del(bg_clouds, c)
			if c.y < 64 then
				add(bg_clouds, cloud:new({x = c.x % (20 * 8) + speed / 2, y = rnd(8), r = rnd(8) + 6}))
			else
				add(bg_clouds, cloud:new({x = c.x % (20 * 8) + speed / 2, y = 128 - rnd(8), r = rnd(8) + 6}))
			end
		end
	end
	for c in all(fg_clouds) do
		c.x -= speed * 3 / 4
		if c.x <= -8 then
			del(fg_clouds, c)
			if c.y < 64 then
				add(fg_clouds, cloud:new({x = c.x % (20 * 8) + speed * 3 / 4, y = rnd(8) - 4, r = rnd(8) + 4}))
			else
				add(fg_clouds, cloud:new({x = c.x % (20 * 8) + speed * 3 / 4, y = 132 - rnd(8), r = rnd(8) + 4}))
			end
		end
	end
end

--end updates

--drawing
function draw_night_sky()
  for s in all(stars) do
		s:draw()
	end
	for c in all(bg_clouds) do
		c:draw(5)
	end
	for c in all(fg_clouds) do
		c:draw(6)
	end
  moon:draw()
end

function draw_label()
  if(time<100)then
    outline_centered("night-sky by jillukowicz",12,6)
  end
end
--end drawing

--pico8 main
function _init()
  music()
  init_clouds()
	init_stars()
  init_moon()
end

function _update()
  time+=1
  update_clouds()
	update_stars()
end

function _draw()
	cls()
  draw_night_sky()
  draw_label()

end
